{
  "rewrites": [
    { "source": "/auth/callback", "destination": "/auth-callback.html" },
    { "source": "/cw-admin-8k3m", "destination": "/admin.html" },
    { "source": "/api/(.*)", "destination": "/api/index.py" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
